{
  "protocol": "https://drchain.info/vcv2/",
  "published": true,
  "types": {
    "vc": {
      "schema": "https://drchain.info/schemas/vcSchema",
      "dataFormats": ["application/vc+jwt"]
    }
  },
  "structure": {
    "vc": {
      "$actions": [
        {
          "who": "anyone",
          "can": "read"
        },
        {
          "who": "anyone",
          "can": "write"
        },
        {
          "who": "author",
          "of": "vc",
          "can": "delete"
        },
        {
          "who": "author",
          "of": "vc",
          "can": "update"
        }
      ]
    }
  }
}
